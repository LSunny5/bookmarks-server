    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "knex": "^0.20.15",
    "morgan": "^1.10.0",
    "pg": "^7.18.2",
    "postgrator": "^4.0.1",
    "uuid": "^7.0.3",
    "valid-url": "^1.0.9",
    "winston": "^3.3.3",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
